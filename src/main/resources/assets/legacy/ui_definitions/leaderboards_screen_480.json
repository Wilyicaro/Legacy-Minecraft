    "boardControlTooltip.y": 12,
    "rankText.x": 10,
    "rankText.y": 20,
    "usernameText.x": 60,
    "usernameText.y": 20,
    "renderableVList.x": "${panel.x}+5",
    "renderableVList.y": "${panel.y}+34",
    "renderableVList.width": "${panel.width}-10",
    "renderableVList.buttonRank.x": 25,
    "renderableVList.buttonUsername.x": 65,
