{
  "targetUI": "wily.legacy.client.screen.LeaderboardsScreen",
  "targetType": "class",
  "applyCondition": { "reference": "uiMode.sd" },
  "elements": {
    "panel.sprite": {
      "type": "put_resource_location",
      "value": "legacy:tiles/panel"
    },
    "panel.width": 328,
    "panel.height": 156,
    "panel.y": "(${height}-${panel.height})/2+6",
    "filterTooltip.width": 96,
    "filterText.y": 10,
    "boardTooltip.width": 110,
    "boardText.y": 4,
    "entriesTooltip.width": 96,
    "entriesText.y": 10,
    "topTooltip.scale": 0.5,
